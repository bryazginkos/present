             xmlns:m='urn:import:present.client.ui.panels'>
        <g:HTMLPanel>
            <table>
                <tr>
                    <td>
                        <m:ItemsEditPanel ui:field="itemsEditPanel"/>
                    </td>
                    <td rowspan="2">
                        <m:ItemsGridPanel ui:field="gridPanel"></m:ItemsGridPanel>
                    </td>
                    <td>
                        <m:PropertiesGridPanel ui:field="propertiesGridPanel"></m:PropertiesGridPanel>
                    </td>
                </tr>
                <tr>
                    <td>
                        <m:FilterPanel></m:FilterPanel>
                    </td>
                    <td></td>
                </tr>
            </table>

        </g:HTMLPanel>
</ui:UiBinder>